<%= bootstrap_form_tag(url: update_proportions_relationship_path(relationship), method: :post, html: {class: 'form-horizontal'}) do |f| %>
    <p>
    <% Visit.kinds.each do |kind, index| %>
        <%= f.number_field kind.to_s+'_per_each' %>
    <% end %>
    </p>
    <p>
      <% Visit.kinds.each do |kind, index| %>
          <%= f.number_field kind.to_s+'_total' %>
      <% end %>
    </p>
    <%= f.submit 'Update', :name => nil %>
<% end %>